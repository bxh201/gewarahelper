{
	"name": "格瓦拉小助手",
	"short_name": "小格助手",
	"description": "去除各种讨厌的提示信息",
	"icons": {
	},
	"manifest_version": 2,
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"browser_action": {
		"default_title": "小格助手-辅助功能",
		"default_popup": "popup.html"
	},
	"permissions": [
		"cookies",
		"tabs",
		"webRequest", "webRequestBlocking",
		"*://*.gewara.com/*",
		"*://*.bypay.cn/*",
		"*://*.alipay.com/*",
		"*://*.cmbchina.com/*"
	],
	"incognito": "split",
	"version": "0.0.4"
}